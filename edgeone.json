{
  "name": "simpage-worker",
  "version": "1.1.0",
  "description": "现代化导航页，由 Cloudflare Workers 驱动",
  "outputDirectory": "./",
  "rewrites": [
    {
      "source": "/",
      "destination": "/public"
    },
    {
      "source": "/api/*",
      "destination": "/node-functions/api/index.js"
    }
  ]
}
